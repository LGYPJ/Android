<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/activity_item_seminar_profile"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:background="@drawable/ic_profile_out"
        android:padding="2dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/item_profile_name_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="알수없음"
            android:textSize="12sp"
            android:textColor="@color/seminar_light_gray"
            android:textAlignment="center"
            android:ellipsize="end"
            android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/activity_item_seminar_profile"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>




    </androidx.constraintlayout.widget.ConstraintLayout>